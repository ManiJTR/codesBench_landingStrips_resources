<!--
        ELEMENT: image.block
        DESCRIPTION: A universal wrapper for one or more images <image> that may optionally include their associated 
    word-searchable content <image.keywords>
        COMMENTS: always required when using an image
 -->
<!ELEMENT image.block (image, image.keywords?, image.text?)>
<!ATTLIST image.block
	rotate (0 | 90 | 180 | 270) #IMPLIED
	position (float | fixed) #IMPLIED
	ID ID #IMPLIED
	protected CDATA #IMPLIED
	modified (true) #IMPLIED
>
<!--
        ELEMENT: image
        DESCRIPTION: Any data object (e.g., photographs, artwork, graphic, drawings, figures, illustrations) rendered in a 
    non-textual graphic format (e.g. tif, bmp, jpeg).
        COMMENTS: 
 -->
<!ELEMENT image EMPTY>
<!ATTLIST image
	ID ID #REQUIRED
	imageID CDATA #REQUIRED
	ident CDATA #REQUIRED
	box (box | no_box) #IMPLIED
	type (photo | form | table | textblock | drawing | xray | map | screenshot | unknown) #IMPLIED
	color (bw | color | graysc) #IMPLIED
	format (bmp | eps | gif | jpeg | pcx | pdf | png | tif) #IMPLIED
	resolut (200 | 300 | 400 | 600) #IMPLIED
	rotate (0 | 90 | 180 | 270) #IMPLIED
	height CDATA #IMPLIED
	width CDATA #IMPLIED
	units (pi | in | cm | mm) #IMPLIED
	scale (scale | noscale) #IMPLIED
	datelastupdated CDATA #IMPLIED
	protected CDATA #IMPLIED
	modified (true) #IMPLIED
>
<!--
        ELEMENT: image.keywords
        DESCRIPTION: This element contains a word-searchable version of the image's content.
        COMMENTS: this is not intended for output in any media. It is purely for "search" purposes in an electronic product 
   and can contain a full-text version of the image or just a simple list of searchable terms
 -->
<!ELEMENT image.keywords (#PCDATA)>
<!--
        ELEMENT: image.text
        DESCRIPTION: This element contains a textual representation of an image.  This might be used
   as a "skeleton" or place-holder
        COMMENTS: 
 -->
<!ELEMENT image.text (codes.head | para | embedded.html)+>
